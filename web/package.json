{
	"name": "web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"//": "The proxy is so supabase can call our auth email webhook from inside the development docker container",
		"dev:proxy": "npx configurable-http-proxy --port 3000 --change-origin --default-target http://localhost:5173",
		"dev": "concurrently --kill-others \"npm run dev:proxy\" \"vite dev\"",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"shad:add": "npx shadcn-svelte add"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4",
		"@internationalized/date": "^3.8.2",
		"@lucide/svelte": "^0.515.0",
		"@playwright/test": "^1.49.1",
		"@storybook/addon-a11y": "^9.0.9",
		"@storybook/addon-docs": "^9.0.9",
		"@storybook/addon-svelte-csf": "^5.0.3",
		"@storybook/addon-vitest": "^9.0.9",
		"@storybook/sveltekit": "^9.0.9",
		"@sveltejs/adapter-cloudflare": "^7.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/node": "^22",
		"@vitest/browser": "3.2.3",
		"@vitest/coverage-v8": "3.2.3",
		"bits-ui": "^2.8.8",
		"concurrently": "^9.2.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"shadcn-svelte": "^1.0.2",
		"storybook": "^9.0.9",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.0.0",
		"tw-animate-css": "^1.3.4",
		"typescript": "^5.0.0",
		"vite": "^6.2.6",
		"vitest": "^3.2.3"
	},
	"dependencies": {
		"@aws-sdk/client-ses": "^3.830.0",
		"@cloudflare/vite-plugin": "^1.7.4",
		"@posthog/ai": "^5.1.0",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.50.0",
		"html-to-text": "^9.0.5",
		"install": "^0.13.0",
		"marked": "^15.0.12",
		"npm": "^11.4.2",
		"openai": "^5.3.0",
		"partial-json": "^0.1.7",
		"posthog-js": "^1.252.0",
		"posthog-node": "^5.1.0",
		"standardwebhooks": "^1.0.0",
		"stripe": "^18.2.1",
		"tailwind-merge": "^3.3.1",
		"zod-deep-partial": "^1.0.0"
	}
}
